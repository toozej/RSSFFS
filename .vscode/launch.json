{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run RSSFFS",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "envFile": "${workspaceFolder}/.env",
            "program": "${workspaceFolder}/main.go",
            "cwd": "${workspaceFolder}",
            "args": ["--category", "photography", "https://blog.jimgrey.net/2024/08/13/film-photography-blogs-you-should-follow-5/"]
        },
        {
            "name": "RSSFFS serve",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "envFile": "${workspaceFolder}/.env",
            "program": "${workspaceFolder}/main.go",
            "cwd": "${workspaceFolder}",
            "args": ["serve", "--debug"]
        },
        {
            "name": "Get RSSFFS version",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "envFile": "${workspaceFolder}/.env",
            "program": "${workspaceFolder}/main.go",
            "cwd": "${workspaceFolder}",
            "args": ["version"]
        }
    ]
}